{
  "manifest_version": 3,
  "name": "Simple Password Manager",
  "version": "1.0",
  "permissions": [
    "storage",
    "clipboardWrite"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
